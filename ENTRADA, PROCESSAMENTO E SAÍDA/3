#include <stdio.h>
int main() {
    float m, dm, cm, mm;

    printf("Digite um valor em metros: ");
    scanf("%f", &m);

    dm = m * 10;
    cm = m * 100;
    mm = m * 1000;

    printf("Valor em decimetros: %.3f\n", dm);
    printf("Valor em centimetros: %.3f\n", cm);
    printf("Valor em milimetros: %.3f\n", mm);

    return 0;
}
